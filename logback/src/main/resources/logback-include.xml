<?xml version="1.0" encoding="utf-8"?>

<!--被包含的文件内容-->
<included>
    <!--控制台输出-->
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d==%contextName === [%thread] %-5level %logger{36}[%file:%line] - %msg%n</pattern>
        </encoder>
    </appender>
</included>